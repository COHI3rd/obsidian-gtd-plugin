{
  "name": "obsidian-gtd-plugin",
  "version": "0.1.0",
  "description": "GTD (Getting Things Done) task management plugin for Obsidian with Japanese UI",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && node -e \"require('fs').copyFileSync('src/styles/main.css', 'styles.css')\"",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "obsidian",
    "gtd",
    "task-management",
    "productivity",
    "japanese"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.5.0",
    "date-fns": "^3.0.6",
    "gray-matter": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
